	~	GWh
	~		|

light duty BEV energy consumption=
	MIN(light duty BEV energy demand, light duty energy demand)/0.65
	~	GWh
	~		|

light duty BEV energy demand= INTEG (
	IF THEN ELSE(light duty efficiency=0.65, potential light duty energy switch , -potential light duty energy switch\
		 ),
		0)
	~	
	~		|

light duty efficiency=
	IF THEN ELSE( MIN( ownership cost BEV urban , ownership cost diesel urban )=ownership cost BEV urban\
		 , 0.65 , 0.35 )
	~	
	~		|

light duty energy demand=
	total energy demand*(1-0.2677)
	~	GWh
	~		|

potential light duty energy switch=
	light duty energy demand*light duty vehicle renovation rate
	~	
	~		|

total energy demand = WITH LOOKUP (
	Time,
		([(0,0)-(10,10)],(2019,867029),(2050,1.401e+06) ))
	~	GWh
	~	\!Year
	|

heavy duty energy demand=
	total energy demand*0.2677
	~	GWh
	~	0.2677 comes from average emissions from heavy duty trucks and buses \
		(Check excel files)
	|

potential heavy duty energy switch=
	heavy duty energy demand*heavy duty vehicle renovation rate
	~	
	~		|

heavy duty efficiency=
	IF THEN ELSE( MIN( ownership cost FCEV long haul , ownership cost diesel long haul )\
		=ownership cost FCEV long haul , 0.65 , 0.35 )
	~	
	~		|

hydrogen price=
	(CAPEX hydrogen+OPEX hydrogen)*(33.33/electrolyser efficiency)
	~	€/kg
	~	33.33 kWh/kg as LHV H2
	|

electrolyser efficiency=
	0.63
	~	
	~		|

CAPEX hydrogen=
	electrolyser cost/electrolyser lifetime
	~	€/kWh
	~		|

electrolyser lifetime=
	60000
	~	h
	~		|

electroliser efficiency=
	0.7
	~	{%}
	~		|

electroliser average working hours=
	4000
	~	h
	~		|

electroliser capacity=
	TOTAL HYDROGEN*33.33/electroliser efficiency/electroliser average working hours/1000
	~	GW
	~	Hydrogen demand (tH2) * 1000 (kgH2/tH2) * 33.33 (kWhH2/kgH2) / efficiency \
		/ working hours (h) *10^-6 (GW/kW)= GW of electroliser capacity
	|

