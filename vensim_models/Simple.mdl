Attractiveness Green=
	Green Demand / Total Demand * 1/Green Price
	~	
	~		|

Attractiveness Grey=
	Grey Demand / Total Demand * 1/Grey Price
	~	
	~		|

Grey Deficit=
	(Grey Demand-Production Grey)
	~	
	~		|

Grey Demand= INTEG (
	Total Demand * (1-Demand Split) + MAX(DELAY1(Grey Deficit, 1 ), 0),
		(1-0.01)*Total Demand)
	~	GWh H2
	~		|

Grey Price=
	(Capacity Grey * (600000)^(2000/Capacity Grey)/2000 * (0.2) + Production Grey * (7000\
		/0.76 + Carbon tax))/Production Grey
	~	€/GWh
	~		|

Subsidies=
	(8.9 * 0.1 * 3.6e+06/120) * 0.5
	~	€/GWh
	~	Half of carbon tax
	|

Total Demand=
	80
	~	GWh H2
	~		|

Demand Split=
	DELAY1( Green Demand , 1 ) * (1 + IF THEN ELSE(Green Price>Grey Price, 0.01 *"Relative attractiveness (Green:Grey)"\
		 , 0 ))/Total Demand
	~	percent [0,1]
	~		|

Investment Green=
	Capacity Green*EXP(-1/"Relative attractiveness (Green:Grey)")
	~	MW H2
	~		|

Production Green=
	Capacity Green*5000/1000
	~	GWh
	~		|

Production Grey=
	Capacity Grey*8500/1000
	~	GWh
	~		|

Progress Ratio Green=
	MAX(0, (1-Maturity Green) * (DELAY1( Production Green , 1 ) - Production Green))
	~	
	~		|

Capacity Green= INTEG (
	DELAY1(Investment Green, 1 ),
		200)
	~	MW H2
	~		|

Investment Grey=
	Capacity Grey*EXP(-1/"Relative attractiveness (Green:Grey)")
	~	MW H2
	~		|

Progress Ratio Grey  = A FUNCTION OF( Maturity Grey,Production Grey) ~~|
Progress Ratio Grey=
	MAX(0, (1-Maturity Grey) * (DELAY1( Production Grey , 1 ) - Production Grey))
	~	
	~		|

Green Deficit=
	Green Demand-Production Green
	~	
	~		|

Maturity Green  = A FUNCTION OF( Progress Ratio Green) ~~|
Maturity Green= INTEG (
	Progress Ratio Green,
		0.05)
	~	[0,1]
	~		|

Green Demand= INTEG (
	Total Demand*Demand Split + MAX(DELAY1(Green Deficit, 1 ), 0),
		0.01*Total Demand)
	~	GWh H2
	~		|

Green Price=
	(Capacity Green*(1.4e+06)^(200/Capacity Green)/200 * (0.18) + Production Green*(30000\
		/0.65 - Subsidies)) / Production Green
	~	€/GWh
	~	18 % annual payment of CAPEX (combined OPEX and CAPEX).
		30000 €/GWh electricity at 65% conversion efficiency.
		Subsidies for every produced GWh.
		Normalized per GWh.
	|

Maturity Grey  = A FUNCTION OF( Progress Ratio Grey) ~~|
Maturity Grey= INTEG (
	(Production Grey - DELAY1(Production Grey, 1 ))/DELAY1(Production Grey, 1 ) * Progress Ratio Grey\
		,
		0.95)
	~	[0,1]
	~		|

"Relative attractiveness (Green:Grey)"=
	0.5 + (LN(Maturity Green) * Attractiveness Green)/(LN(Attractiveness Grey) * Maturity Grey\
		)
	~	
	~		|

